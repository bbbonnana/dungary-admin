{
  "name": "dungary-admin",
  "version": "1.0.0",
  "description": "dungary admin",
  "main": "index.js",
  "repository": "https://github.com/bbbonnana/dungary-web.git",
  "author": "LiNKS <a824759414@gmail.com>",
  "license": "MIT",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "scripts": {
    "lint": "eslint --ext .js,.vue .",
    "lint:fix": "eslint --ext .js,.vue . --fix",
    "serve": "npm run serve:local",
    "serve:local": "cross-env MODE=dev APP_ENV=local webpack-dev-server --progress --config build/serverWebpackConfig.js",
    "serve:dev": "cross-env MODE=dev APP_ENV=dev webpack-dev-server --progress --config build/serverWebpackConfig.js",
    "serve:prod": "cross-env MODE=dev APP_ENV=prod webpack-dev-server --progress --config build/serverWebpackConfig.js",
    "build:local": "cross-env MODE=dev APP_ENV=local node build/build.js",
    "build:dev": "cross-env MODE=prod APP_ENV=dev node build/build.js",
    "build:prod": "cross-env MODE=prod APP_ENV=prod node build/build.js"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11",
    "vue-router": "^3.4.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.1.0",
    "chalk": "^4.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.2.1",
    "eslint": "^7.8.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "postcss-loader": "^3.0.0",
    "rimraf": "^3.0.2",
    "sass": "^1.26.10",
    "sass-loader": "^10.0.2",
    "style-loader": "^1.2.1",
    "url-loader": "^4.1.0",
    "vue-loader": "^15.9.3",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.1"
  }
}
